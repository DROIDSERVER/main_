


error_reporting(0);

const

host   = ['Adbeast','Adbeast.org',''],

version="1.0",

cookie = "cookie.".host[0],

useragent= "user_agent",

web    = "https://".host[1];

Function h(){

    $h = array();

    $h[] = "Host: ".host[1];

    $h[] = "X-requested-with: XMLHttpRequest";

    $h[] = "Accept: text/html,application/xhtml+xml,application/xml;q=0.9,image/avif,image/webp,image/apng,*/*;q=0.8,application/signed-exchange;v=b3;q=0.7";

    $h[] = "Accept-language: id-ID,id;q=0.9,en-US;q=0.8,en;q=0.7,ja;q=0.6";

    $h[] = "Cookie: ".file_get_contents(cookie);

    $h[] = "User-Agent: ".file_get_contents(useragent);

    return $h;

}

Function balance(){

    $r = get(web);

    $balance = Ambil($r,'Account Balance <div class="text-primary"><b>','</',1);

    $value = Ambil($r,'Coins Value <div class="text-success"><b>','<',1);

    return w3.$balance.p."/".w3.$value;

}

Function Suks($lucky,$reward, $balance){

    a_claim();

    echo cek("âˆš").p."LuckyNum".panah.w3.$lucky.p."/".w3.$reward." coins".n;

    echo cek("+").p."Balance ".panah.w3.$balance.n;

}

Function usn(){

    $r = get(web);

    $u = Ambil($r,'<font class="text-success">','</font><br />',1);

    return w3.$u;

}

