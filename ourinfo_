Function LPP(){

    TimeZone();

    check_key:

    cl();ban();

    $r=file_get_contents('https://xeidkdseotiba.blogspot.com');

    $ambil= Ambil($r,"<scrypt duit='","'/>",1);

    $data = kunci($ambil);

    $date    = date("D");

    $Mentah  = Ambil($data,$date." [ "," | ",1);

    $Short   = Ambil($data,"$Mentah | "," ]",1);

    if(!file_exists("key")){

        echo " ".p.str_pad("Link : ".w3.$Short, 50, " ", STR_PAD_BOTH).n;

        echo p." ".w3;

        $p= trim(fgets(STDIN));

        if($pass=$p){ 

            $save = fopen("key", "w");

            fwrite($save, $p);

            fclose($save);

        }else{

            echo " ".m.str_pad("Password is incorrect ", 50, " ", STR_PAD_BOTH).n;

            sleep(3);

            print r;

            goto check_key;

        }  

    }

    $res = file_get_contents($Mentah);

    $pass= Ambil($res,'<p>Password : ','</p>',1);

    $read=file_get_contents('key');

    if($read == $pass){

        echo r;

        echo " ".w3.str_pad("Password is correct ✅", 50, " ", STR_PAD_BOTH).n;

        sleep(3);

        print r;

    }elseif($read !=$pass){

        echo " ".p.str_pad("Link : ".w3.$Short, 50, " ", STR_PAD_BOTH).n;

        echo p." ".w3;

        $p= trim(fgets(STDIN));

        if($p ==''){goto check_key;} 

        if($pass == $p){

                $save = fopen("key", "w");

                fwrite($save, $p);

                print r;

                echo " ".w3.str_pad("Password is correct ✅", 50, " ", STR_PAD_BOTH).n;

                fclose($save);

                sleep(3);

                print r;

        }else{

            echo " ".m.str_pad("Password is incorrect ", 50, " ", STR_PAD_BOTH).n;

            sleep(3);

            goto check_key;

        }    

    }   

}



Function HCap($api_url,$apikey, $sitekey, $pageurl,$delay){

	$r =  json_decode(file_get_contents($api_url."/in.php?key=".$apikey."&method=hcaptcha&sitekey=".$sitekey."&pageurl=".$pageurl."&json=1"),1);

	$status = $r["status"];

	if($status == 0){print(" Apikey: ".k.$r["request"].n.p);return 0;}

	$id = $r["request"];

	while(true){

		print lo;

		$r = json_decode(file_get_contents($api_url."/res.php?key=".$apikey."&action=get&id=".$id."&json=1"),1);

		$status = $r["status"];

		if($r["request"] == "CAPCHA_NOT_READY"){

			echo "\r                      \r";

			print lo1;

			sleep($delay);

			print "\r                    \r";

			continue;

		}

		if($status == 1){print "\r                 \r";return $r["request"];}

		return 0;

	}

}

Function Rv2($api_url,$apikey, $sitekey, $pageurl,$delay){

    $r =  json_decode(file_get_contents($api_url."/in.php?key=".$apikey."&method=userrecaptcha&googlekey=".$sitekey."&pageurl=".$pageurl."&json=1"),1);

    $status = $r["status"];

    if($status == 0){print(" Apikey: ".k.$r["request"].n);return 0;}

    $id = $r["request"];

    while(true){

        print "\r                    \r";

        print lo;

        $r = json_decode(file_get_contents($api_url."/res.php?key=".$apikey."&action=get&id=".$id."&json=1"),1);

        $status = $r["status"];

        if($r["request"] == "CAPCHA_NOT_READY"){

        print "\r                    \r";

        print lo1;

        sleep($delay);

        print "\r                    \r";

        continue;

        }

        if($status == 1){print "\r                    \r";return $r["request"];}

        return 0;

    }

}



Function anti_bot($source,$api_url,$apikey,$delay){

	$main = explode('"',explode('<img src="',explode('Bot links',$source)[1])[1])[0];

	$antiBot["main"] = $main;

	$src = explode('rel=\"',$source);

	foreach($src as $x => $sour){

		if($x == 0)continue;

		$no = explode('\"',$sour)[0];

		$img = Ambil($sour,'<img src=\"','\"',1);

		$antiBot[$no] = $img;

	}

	$ua = "Content-type: application/x-www-form-urlencoded";

	$data = ["key"=>$apikey,"method"=>"antibot","json"=>1] + $antiBot;

	$opts = ['http' =>['method'  => 'POST','header' => $ua,'content' => http_build_query($data)]];

	

	$r = json_decode(file_get_contents($api_url.'/in.php', false, stream_context_create($opts)),1);

	$status = $r["status"];

	if($status == 0){

		print(p." Apikey: ".k.$r["request"].n);

		return 0;

	}

	$id = $r["request"];

	while(true){

		print lo;

		$r = json_decode(file_get_contents($api_url."/res.php?key=".$apikey."&action=get&id=".$id."&json=1"),1);

		$status = $r["status"];

		if($r["request"] == "CAPCHA_NOT_READY"){

			print "\r                      \r";

			print lo1;

			sleep($delay);

			print "\r                    \r";

			continue;

		}

		if($status == 1){print "\r                 \r";return "+".str_replace(",","+",$r["request"]);}

		return 0;

	}

}

Function Api_Bal($api_url){

    $apikey = file_get_contents("Apikey");

	$r = json_decode(file_get_contents($api_url."/res.php?action=userinfo&key=".$apikey),1);

    if(!$r["balance"]){print (" Apikey: ".w3." Saldo Apikey habis!".n);}return $r["balance"];

}

Function RecaptchaV3($anchor){

    while(true){

        $r = curl($anchor,array());

        $token = Ambil($r,'<input type="hidden" id="recaptcha-token" value="','">',1);

        $sitekey = explode("&",$anchor)[1];

        $co = explode("&",$anchor)[2];

        $v = explode("&",$anchor)[4];

        $r = curl("https://www.google.com/recaptcha/api2/reload?".$sitekey,array(),"$v&reason=q&c=$token&$v&$co");

        $res = explode('"',explode('["rresp","',$r)[1])[0];

        if($res){return $res;}

    }

}

