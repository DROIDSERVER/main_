Function Menu($no, $str){print w3." [".p.$no.w3."] ".p.$str.n;}

Function Select($str){return w3." [".p."Input ".$str.w3."] ".n." ".p;}

Function His($newdata,$data=0){if(!$data){$data = [];}return array_merge($data,$newdata);}

Function Captcha($api_url,$apikey, $sitekey, $pageurl,$delay){

    global $r;

    $x=$r;

    if(preg_match("/h-captcha/",$x)){$r =  json_decode(file_get_contents($api_url."/in.php?key=".$apikey."&method=hcaptcha&sitekey=".$sitekey."&pageurl=".$pageurl."&json=1"),1);}

    if(preg_match("/g-recaptcha/",$x)){$r =  json_decode(file_get_contents($api_url."/in.php?key=".$apikey."&method=userrecaptcha&googlekey=".$sitekey."&pageurl=".$pageurl."&json=1"),1);}

    if(preg_match("/cf-turnstile/",$x)){$r =  json_decode(file_get_contents($api_url."/in.php?key=".$apikey."&method=turnstile&sitekey=".$sitekey."&pageurl=".$pageurl."&json=1"),1);}

    $status = $r["status"];

        if($status == 0){print(" Apikey: ".k.$r["request"].n.p);return 0;}

        $id = $r["request"];

        while(true){

            print lo;

            $r = json_decode(file_get_contents($api_url."/res.php?key=".$apikey."&action=get&id=".$id."&json=1"),1);

            $status = $r["status"];

            if($r["request"] == "CAPCHA_NOT_READY"){

                print rr;

                print lo1;

                sleep($delay);

                print rr;

                continue;

            }

            if($status == 1){print rr;return $r["request"];}

            return 0;

        }

}


